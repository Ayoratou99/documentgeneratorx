{
    "name": "ayoratoumvone/documentgeneratorx",
    "description": "A package to generate documents (DOCX/PDF) based on templates with advanced variable types including images",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "document-generator",
        "template",
        "docx",
        "pdf",
        "document-generation",
        "phpword",
        "dompdf"
    ],
    "authors": [
        {
            "name": "Ayoratou99",
            "email": "44085615+Ayoratou99@users.noreply.github.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "phpoffice/phpword": "^1.2",
        "dompdf/dompdf": "^2.0",
        "intervention/image": "^3.0",
        "setasign/fpdi": "^2.6",
        "tecnickcom/tcpdf": "^6.10"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0",
        "laravel/pint": "^1.13"
    },
    "autoload": {
        "psr-4": {
            "Ayoratoumvone\\Documentgeneratorx\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ayoratoumvone\\Documentgeneratorx\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ayoratoumvone\\Documentgeneratorx\\DocumentGeneratorServiceProvider"
            ],
            "aliases": {
                "DocumentGenerator": "Ayoratoumvone\\Documentgeneratorx\\Facades\\DocumentGenerator"
            }
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "format": "vendor/bin/pint"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
